issn = {1088-9051},
journal = {Genome Research},
month = dec,
pages = {gr.131383.111--},
title = {{GAGE: A critical evaluation of genome assemblies and assembly algorithms}},
url = {http://genome.cshlp.org/cgi/content/abstract/gr.131383.111v1},
year = {2011}
}
