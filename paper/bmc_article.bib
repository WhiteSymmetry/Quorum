pmid = {12529310},
title = {{Whole-genome sequence assembly for mammalian genomes: Arachne 2.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=430950\&tool=pmcentrez\&rendertype=abstract},
