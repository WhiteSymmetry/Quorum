issn = {1549-5469},
journal = {Genome research},
month = may,
number = {7},
pages = {1181--1192},
pmid = {21482625},
title = {{ECHO: A reference-free short-read error correction algorithm.}},
url = {http://genome.cshlp.org/cgi/content/abstract/21/7/1181},
volume = {21},
year = {2011}
}
